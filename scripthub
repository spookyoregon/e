-- Scripts

local function DQYK_fake_script() -- Home.LocalScript 
	local script = Instance.new('LocalScript', Home)

	local button = script.Parent
	local settingsframe = button.Parent.Parent.SettingElement.ScrollingFrame
	local homeframe = button.Parent.Parent.HomeElement.Frame
	local ButtonClick = script.Parent.Parent.Parent.Audios.ButtonClicks
	local visualframe = button.Parent.Parent.VisualsElement.Holder
	local scriptelement = button.Parent.Parent.ScriptElement.ScriptsHolder
	
	button.MouseButton1Click:Connect(function()
		if settingsframe.Visible then
			ButtonClick:Play()
			settingsframe.Visible = false
			homeframe.Visible = true
		else
		if visualframe.Visible then
			ButtonClick:Play()
			visualframe.Visible = false
			homeframe.Visible = true
		else
			if scriptelement.Visible then
				ButtonClick:Play()
				scriptelement.Visible = false
				homeframe.Visible = true
		else
			ButtonClick:Play()
			homeframe.Visible = true
				end
			end
		end
	end)
	
end
coroutine.wrap(DQYK_fake_script)()
local function BHAZQN_fake_script() -- Scripts.LocalScript 
	local script = Instance.new('LocalScript', Scripts)

	local ButtonClick = script.Parent.Parent.Parent.Audios.ButtonClicks
	local button = script.Parent
	local scriptelement = button.Parent.Parent.ScriptElement.ScriptsHolder
	local homeelement = button.Parent.Parent.HomeElement.Frame
	local settingselement = button.Parent.Parent.SettingElement.ScrollingFrame
	local visualselement = button.Parent.Parent.VisualsElement.Holder
	
	button.MouseButton1Click:Connect(function()
		if homeelement.Visible then
			ButtonClick:Play()
			homeelement.Visible = false
			scriptelement.Visible = true
		else
			if settingselement.Visible then
				ButtonClick:Play()
				settingselement.Visible = false
				scriptelement.Visible = true
			else
				if visualselement.Visible then
					ButtonClick:Play()
					visualselement.Visible = false
					scriptelement.Visible = true
				else
					ButtonClick:Play()
					scriptelement.Visible = true
				end
			end
		end
	end)
end
