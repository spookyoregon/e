local function GFSD_fake_script() -- button1_3.LocalScript 
	local script = Instance.new('LocalScript', button1_3)

	local button = script.Parent
	local bluecolor = Color3.fromRGB(17, 95, 212)
	local imagelabel = button.checkmark
	
	button.MouseButton1Click:Connect(function()
		local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/marssaysok/skeleton/main/esp"))()
	
		local Skeletons = {}
		local localPlayer = game.Players.LocalPlayer
	
		for _, Player in next, game.Players:GetChildren() do
			if Player ~= localPlayer then
				table.insert(Skeletons, Library:NewSkeleton(Player, true))
			end
		end
	
		game.Players.PlayerAdded:Connect(function(Player)
			if Player ~= localPlayer then
				table.insert(Skeletons, Library:NewSkeleton(Player, true))
			end
		end)
			button.BackgroundColor3 = bluecolor
			imagelabel.Visible = true
	end)
	
end
coroutine.wrap(GFSD_fake_script)()
