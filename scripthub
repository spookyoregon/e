local function MEAH_fake_script() -- button1.LocalScript 
	local script = Instance.new('LocalScript', button1)

	local button = script.Parent
	local bluecolor = Color3.fromRGB(17, 95, 212)
	local imagelabel = button.checkmark
	
	local function disableShadows()
		game:GetService("Lighting").GlobalShadows = false
	end
	
	local function enableShadows()
		game:GetService("Lighting").GlobalShadows = true
	end
	
	button.MouseButton1Click:Connect(function()
		if button.BackgroundColor3 == bluecolor and imagelabel.Visible == true then
			button.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			imagelabel.Visible = false
			enableShadows()
		else
			button.BackgroundColor3 = bluecolor
			imagelabel.Visible = true
			disableShadows()
		end
	end)
	
end
