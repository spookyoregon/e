local function XASL_fake_script() -- button2.LocalScript 
	local script = Instance.new('LocalScript', button2)

	local button = script.Parent
	local bluecolor = Color3.fromRGB(17, 95, 212)
	local imagelabel = button.checkmark
	
	local function fpsboost()
		local fps = 999
		if setfpscap then
			setfpscap(fps)
		end
	end
	
	local function fpsstay()
		local fps = 60
		if setfpscap then
			setfpscap(fps)
		end
	end
	
	button.MouseButton1Click:Connect(function()
		if button.BackgroundColor3 == bluecolor and imagelabel.Visible == true then
			button.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			imagelabel.Visible = false
			fpsstay()
		else
			button.BackgroundColor3 = bluecolor
			imagelabel.Visible = true
			fpsboost()
		end
	end)
	
end
coroutine.wrap(XASL_fake_script)()
local function KMZAI_fake_script() -- button3.LocalScript 
	local script = Instance.new('LocalScript', button3)

	local button = script.Parent
	local bluecolor = Color3.fromRGB(17, 95, 212)
	local imagelabel = button.checkmark
	
	local function disableAllSounds()
		for _, sound in pairs(game:GetService("SoundService"):GetChildren()) do
			if sound:IsA("Sound") then
				sound.Volume = 0
			end
		end
	end
	
	local function enableAllSounds()
		for _, sound in pairs(game:GetService("SoundService"):GetChildren()) do
			if sound:IsA("Sound") then
				sound.Volume = 1
			end
		end
	end
	
	local function toggleButtonState()
		if button.BackgroundColor3 == bluecolor then
			button.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			imagelabel.Visible = false
			enableAllSounds()
		else
			button.BackgroundColor3 = bluecolor
			imagelabel.Visible = true
			disableAllSounds()
		end
	end
	
	button.MouseButton1Click:Connect(toggleButtonState)
	
end
coroutine.wrap(KMZAI_fake_script)()
local function XDAF_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local player = game.Players.LocalPlayer
	
	script.Parent.Changed:Connect(function()
		local scale = script.Parent.Position.X.Scale
		local number = math.floor(scale*100)
		local volume = number
		local camera = game.Workspace.CurrentCamera
		if camera then
			camera.FieldOfView = 70 + (0.60*volume)
		end
	end)
end
coroutine.wrap(XDAF_fake_script)()
local function GBGX_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	local Player = game:GetService("Players").LocalPlayer
	local UIS = game:GetService("UserInputService")
	local Runservice = game:GetService("RunService")
	local Frame = script.Parent
	local Button = script.Parent.TextButton
	local db = false
	local step = 0.01
	local percentage = 0
	
	function snap(number,factor)
		if factor == 0 then
			return number
		else
			return math.floor(number/factor+0.5)*factor
	
		end
	end
	
	UIS.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			db = false
		end
	end)
	
	script.Parent.TextButton.MouseButton1Down:Connect(function()
		db = true
	end)
	
	Runservice.RenderStepped:Connect(function()
		if db then
			local MousePos = UIS:GetMouseLocation().X
			local BtnPos = Button.Position
			local FrameSize = Frame.AbsoluteSize.X
			local FramePos = Frame.AbsolutePosition.X
			local pos = snap((MousePos-FramePos)/FrameSize,step)
			percentage = math.clamp(pos,0,0.85)
			Button.Position = UDim2.new(percentage,0,BtnPos.Y.Scale,BtnPos.Y.Offset)
		end
	end)
end
coroutine.wrap(GBGX_fake_script)()
